<h1>Welcome to the Workshop Collective</h1>
<a href="/sheds/new">New Shed</a>


<h2>One Shed, Many Tools</h2>
<% @sheds.each do |shed| %>
  
    <h3><a href="/sheds/<%= shed.id %>"><%= shed.name %></a></h3>
    <p><%= button_to "Update #{shed.name}", "/sheds/#{shed.id}/edit", method: :get %></p>
    <p>Hosting tools since: <%= shed.created_at %></p>

<% end %>




